{
  "vroom": {
    "port": 3000,
    "osrm_url": "http://osrm:5000",
    "max_jobs": 1000,
    "max_vehicles": 100,
    "timeout": 30000
  },
  "logging": {
    "level": "info",
    "format": "json"
  },
  "cors": {
    "origin": "*",
    "methods": ["GET", "POST", "PUT", "DELETE"],
    "headers": ["Content-Type", "Authorization"]
  }
} 